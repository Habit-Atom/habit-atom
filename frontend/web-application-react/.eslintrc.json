{
  "env": {
      "browser": true,
      "webextensions": true,
      "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "rules": {
      // Disable the 'no-unused-vars' rule by setting it to 'off'
      "no-unused-vars": "off"
  }
}